public class HotelManagerFactory extends AbstractFactory{

	public function	AbstractHotelManager getHotelManager($hotelType)
	{
	    switch($hotelType) {
	      case "ITC":
	      	return new ITCHotelManager();
	      	break;
	      case "AH":
	        return new AHHOtelManager();
	        break;
	      case "TES":
	        return new TESHOtelManager();
	        break;
	    }
	}
}